{
    "name": "xkzl/skeleton-bundle",
    "type": "symfony-bundle",
    "description": "Skeleton integration for Symfony",
    "keywords": ["symfony", "skeleton"],
    "homepage": "https://gitlab.glitchr.io/repository-public/symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyerconde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "xKzl\\SkeletonBundle\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\xKzl\\SkeletonBundle\\": "./Tests"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "^4.4.17|^5.0",
        "symfony/dependency-injection": "^4.4.17|^5.0",
        "symfony/http-kernel": "^4.4.17|^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}

