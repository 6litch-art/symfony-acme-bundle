{
    "name": "xkzl/acme-bundle",
    "type": "symfony-bundle",
    "description": "ACME Skeleton Bundle for Symfony5",
    "keywords": ["symfony", "skeleton", "acme"],
    "homepage": "https://gitlab.glitchr.io/repository-public/symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyerconde@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Acme\\Bundle\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Acme\\Bundle\\": "./src/Tests"
        }
    },
    "require": {
        "php": ">=7.2.5",
        "symfony/config": "5.3.*",
        "symfony/dependency-injection": "5.3.*",
        "symfony/http-kernel": "5.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
